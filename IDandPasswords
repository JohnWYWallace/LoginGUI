import java.util.HashMap;

public class IDandPasswords {
	
	HashMap<String,String> logininfo = new HashMap<String,String>();
	
	IDandPasswords(){
		
		logininfo.put("johnwallace", "1234");   //first you type in the name of the HashMap
		logininfo.put("blakepartin", "1234");
		
	}
	
	// For this method the return type is HashMap and is protected so not just any one can call it
	protected HashMap getLoginInfo(){
		
		return logininfo;		//whichever method calls this method will send the login info 
		
	}
	

}

